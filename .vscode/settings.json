{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "cssvar.files": ["front-end/src/styles/global/variables.css"],
  "cssvar.extensions": ["css", "scss"],
  "[elixir]": {
    "editor.defaultFormatter": "JakeBecker.elixir-ls",
    "editor.formatOnType": true,
